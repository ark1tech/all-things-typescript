{
  "compilerOptions": {
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "baseUrl": ".",
    "paths": {
      "@public/*": [
        "./public/*"
      ],
      "@/*": [
        "./src/*"
      ],
      "@components/*": [
        "./src/components/*"
      ]
    },
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ]
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    ".next/types/**/*.ts"
, "src/app/(routes)/[id]/[recipeName]/_components/RecipeDetails/HomeRecipeNav"  ],
  "exclude": [
    "node_modules"
  ]
}